<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>优书网</os:ShortName>
<os:Description>优书网</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://www.yousuu.com</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.yousuu.com/" resultDomain="yousuu.com">
