<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>CodeGuru</os:ShortName>
<os:Description>CodeGuru - Microsoft developers related ideas, articles, tips, tricks, comments, downloads, and so much more related to programming in areas including C++, Visual C++, C#, Visual Basic, .NET Framework, and more</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://www.codeguru.com</SearchForm>
<os:Url type="text/html" method="POST" template="https://www.codeguru.com/search/" resultDomain="codeguru.com">
  <os:Param name="query" value="{searchTerms}"/>
  <os:Param name="btnSearch" value="Go"/>
</os:Url>
<UpdateUrl>https://raw.githubusercontent.com/zephyrer/opensearch/master/codeguru.xml</UpdateUrl>
<UpdateInterval>7</UpdateInterval>
</SearchPlugin>